{
  "compilerOptions": {
    "checkJs": false,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "*": ["*", "src/*"]
    },
    "noEmit": true,
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": false,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "build", "dist", "**/node_modules/*"],
  "include": ["frontend/src/**/*"]
}
